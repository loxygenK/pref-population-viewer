{
  "name": "pref-population-viewer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "lint": "$npm_execpath lint:eslint && $npm_execpath lint:prettier",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier ./src/**/*.{ts,tsx,module.scss} -c",
